{
  "name": "tmb_poc",
  "version": "1.0.0",
  "description": "POC for extracting pallet movement data from German logistics documents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "server": "tsx src/server.ts",
    "extract": "tsx src/index.ts",
    "extract:batch": "tsx src/batch.ts",
    "generate:excel": "tsx src/generate-excel.ts",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "prepare": "husky"
  },
  "keywords": [
    "logistics",
    "pallet",
    "extraction",
    "ocr"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@t3-oss/env-core": "^0.13.10",
    "ai": "^6.0.3",
    "dotenv": "^17.2.3",
    "hono": "^4.11.3",
    "pdf-to-img": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
