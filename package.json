{
  "name": "tmb_poc",
  "version": "1.0.0",
  "description": "POC for extracting pallet movement data from German logistics documents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "extract": "tsx src/index.ts",
    "extract:batch": "tsx src/batch.ts",
    "generate:excel": "tsx src/generate-excel.ts",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "prepare": "husky"
  },
  "keywords": [
    "logistics",
    "pallet",
    "extraction",
    "ocr"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^6.0.3",
    "dotenv": "^17.2.3",
    "pdf-to-img": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
